#!/bin/zsh

function fndut() #for youtube video only
{
	echo "[SCRIPT]>>START"
	for id in "$@"; do
		if [[ $id = http*://*youtu*be*/*watch\?v=[-_a-zA-Z0-9]* ]]; then
			id="${id#http*://*youtu*be*/*watch\?v=}"
		elif [[ $id = http*://*youtu*be*/[-_a-zA-Z0-9]* ]]; then
			id="${id#http*://*youtu*be*/}"

# bash: ${$1#http*://*youtu*be*/*watch\?v[=]}: bad substitution
# bash: ${"$1"#http*://*youtu*be*/*watch\?v[=]}: bad substitution
		fi
		echo "Search for ID [$id] now..."
		sudo find / -iname "*$id*"
	done
	echo "[SCRIPT]>>END"
}

function srvstr()
{
	if [ $# -eq 1 ]; then
		sudo service "$1" start
	fi
}

function srvstp()
{
	if [ $# -eq 1 ]; then
		sudo service "$1" stop
	fi
}

function lpr()
{
	if [ $# -eq 0 ]; then
		ll *part
	else
		if [[ $1 = */ ]]; then
			ll "$1"*part
		else
			ll "$1"/*part
		fi
	fi
}

function utmp3()
{
        youtube-dl -x --audio-format mp3 --audio-quality 0 "$1" #to keep video: -k
}

function ut18()
{
        youtube-dl -f 18 "$1"
}

