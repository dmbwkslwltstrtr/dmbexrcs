wget -q -O - https://www.kali.org/archive-key.asc | gpg --import
wget http://cdimage.kali.org/kali-2016.1/SHA1SUMS.gpg
wget http://cdimage.kali.org/kali-2016.1/SHA1SUMS
gpg --verify SHA1SUMS.gpg SHA1SUMS

